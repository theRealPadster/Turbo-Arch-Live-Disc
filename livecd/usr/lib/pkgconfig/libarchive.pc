prefix=/usr
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: libarchive
Description: library that can create and read several streaming archive formats
Version: 3.0.3
Cflags: -I${includedir}
Libs: -L${libdir} -larchive
Libs.private: -lacl -lattr -lexpat -llzma -lbz2 -lz   -lcrypto
