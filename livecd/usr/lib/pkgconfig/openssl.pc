prefix=/usr
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: OpenSSL
Description: Secure Sockets Layer and cryptography libraries and tools
Version: 1.0.0g
Requires: 
Libs: -L${libdir} -lssl -lcrypto -Wl,-O1,--sort-common,--as-needed,-z,relro,--hash-style=gnu -ldl -lz
Cflags: -I${includedir} 
